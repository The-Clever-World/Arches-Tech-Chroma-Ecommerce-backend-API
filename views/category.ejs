          <br>

          <div class="form">
            <h1 class="text-center">Registration Form</h1>
            <!-- Progress bar -->
            <div class="progressbar">
              <div class="progress" id="progress"></div>

              <div class="progress-step progress-step-active" data-title="Intro"></div>
              <div class="progress-step" data-title="Contact"></div>
              <div class="progress-step" data-title="ID"></div>
              <div class="progress-step" data-title="Password"></div>
            </div>

            <!-- Steps -->
            <div class="form-step form-step-active">
              <div class="input-group">
                <label for="username">Full Name</label>
                <input style="width: 100%;" type="text" name="username" id="username" />
              </div>
              <div class="input-group">
                <label for="position"> Username</label>
                <input style="width: 100%;" type="text" name="position" id="position" />
              </div>
              <div class="">
                <a href="#" class="btn btn-next width-50 ml-auto">Next</a>
              </div>
            </div>
            <div class="form-step">
              <div class="input-group">
                <label for="phone">Phone</label>
                <input style="width: 100%;" type="text" name="phone" id="phone" />
              </div>
              <div class="input-group">
                <label for="email">Email</label>
                <input style="width: 100%;" type="text" name="email" id="email" />
              </div>
              <div class="btns-group">
                <a href="#" class="btn btn-prev">Previous</a>
                <a href="#" class="btn btn-next">Next</a>
              </div>
            </div>
            <div class="form-step">
              <div class="input-group">
                <label for="dob">Date of Birth</label>
                <input style="width: 100%;" type="date" name="dob" id="dob" />
              </div>
              <div class="input-group">
                <label for="ID">National ID</label>
                <input style="width: 100%;" type="number" name="ID" id="ID" />
              </div>
              <div class="btns-group">
                <a href="#" class="btn btn-prev">Previous</a>
                <a href="#" class="btn btn-next">Next</a>
              </div>
            </div>
            <div class="form-step">
              <div class="input-group">
                <label for="password">Password</label>
                <input style="width: 100%;" type="password" name="password" id="password" />
              </div>
              <div class="input-group">
                <label for="confirmPassword">Confirm Password</label>
                <input style="width: 100%;" type="password" name="confirmPassword" id="confirmPassword" />
              </div>
              <div class="btns-group">
                <a href="#" class="btn btn-prev">Previous</a>
                <button class="btn btn-primary" id="register">Submit</button>
              </div>
            </div>
          </div>
          <div class="text-center think-link" data-toggle="modal" data-target="#staticBackdrop_2">Click here
            for all ready Login User <a href="#" style="color:blue;">Login</a></div>

          <div class="modal-footer d-flex justify-content-end">
            <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
          </div>

        </div>
      </div>
    </div>
  </div>



