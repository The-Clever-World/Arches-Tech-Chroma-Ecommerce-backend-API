                            </div>

                            <h5>Key Features</h5>
                            <hr>
                            <div class="form-group">
                              <label for="exampleInputEmail1">Key Feature 1</label>
                              <input type="text" class="form-control" id="Key-Feature-1"
                                placeholder="Enter key feature of product">
                            </div>
                            <div class="form-group">
                              <label for="exampleInputEmail1">Key Feature 2</label>
                              <input type="text" class="form-control" id="Key-Feature-2"
                                placeholder="Enter key feature of product">
                            </div>
                            <div class="form-group">
                              <label for="exampleInputEmail1">Key Feature 3</label>
                              <input type="text" class="form-control" id="Key-Feature-3"
                                placeholder="Enter key feature of product">
                            </div>
                            <div class="form-group">
                              <label for="exampleInputEmail1">Key Feature 4</label>
                              <input type="text" class="form-control" id="Key-Feature-4"
                                placeholder="Enter key feature of product">
                            </div>
                            <h5>Images</h5>
                            <hr>
                            <div class="form-group">
                              <label for="exampleInputEmail1">Image 1</label>
                              <input type="text" class="form-control" id="Image-1" placeholder="Enter product image">
                            </div>
                            <div class="form-group">
                              <label for="exampleInputEmail1">Image 2</label>
                              <input type="text" class="form-control" id="Image-2" placeholder="Enter product image">
                            </div>
                            <div class="form-group">
                              <label for="exampleInputEmail1">Image 3</label>
                              <input type="text" class="form-control" id="Image-3" placeholder="Enter product image">
                            </div>
                            <div class="form-group">
                              <label for="exampleInputEmail1">Image 4</label>
                              <input type="text" class="form-control" id="Image-4" placeholder="Enter product image">
                            </div>
                            <h5>Price and Stocks</h5>
                            <hr>
                            <div class="form-group">
                              <label for="exampleInputEmail1">Price in rupees</label>
                              <input type="text" class="form-control" id="price" placeholder="Enter price">
                            </div>
                            <div class="form-group">
                              <label for="exampleInputEmail1">Stock count</label>
                              <input type="number" class="form-control" id="stock"
                                placeholder="Enter total number of items available">
                            </div>

                            <br/>
                            
                            <!-- Category dropdown -->
                            <h5>Category</h5>
                            <hr  />
                            <h6>Choose category from dropdown</h6>
                            <div class="form-group">
                              <select class="mb-3 form-control form-control-lg">
                                <% categories.forEach((category) => { %>
