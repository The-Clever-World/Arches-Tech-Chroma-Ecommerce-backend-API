      <section id="" class="home-product-slider">
         <div class="container">
            <div class="owl-carousel owl-theme home-product-owl-sliders">

               <!-- List of Products of particular category -->

               <% category.products.forEach((product)=> { %>

                  <a href="/products/<%= product._id %>">
                     <div class="item">
                        <div class="home-product-image-slider">
                           <figure>
                              <img src="/public/<%= product.images[0] %>" />
                           </figure>
                        </div>
                        <div class="item-cart">
                           <i class="fa fa-heart-o" aria-hidden="true"></i>
                        </div>
                        <div class="home-product-content-slider">
                           <div class="slider-content">
                              <p class="category-title">
                                 <%= product.name %>
                              </p>
                              <div class="category-price-div">
                                 <span class="category-count">
                                    <b> ₹<%= product.price %> </b>
                                 </span>
                                 <span class="old-price">
                                    <del>₹<%= product.price + 1000 %> </del>
                                 </span>
                              </div>
                              <div class="home-slider-star">
                                 <% let integer=product.rating.toString().split(".")[0] %>
                                    <% let decimal=product.rating.toString().split(".")[1] %>

                                       <% for( let i=0; i < integer; i++ ) { %>
                                          <i style="float: left" class="fa fa-star"></i>
                                          <% } %>

                                             <% if (decimal && decimal>0) { %>
                                                <i class="fa fa-star-half-o"></i>
                                                <% } %>

                                                   <p>(<%= product.numReviews %> Review)</p>
                              </div>
                           </div>
                        </div>
                     </div>
                  </a>

                  <% }) %>

            </div>
         </div>
      </section>

      <% }) %>

